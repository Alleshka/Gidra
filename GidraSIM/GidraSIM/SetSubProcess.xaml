<Window x:Class="GidraSIM.SetSubProcess"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Выбор процесса" Height="220" Width="300" FontSize="16" ResizeMode="NoResize">
    <Window.Background>
        <ImageBrush ImageSource="/GidraSIM;component/Image/background.jpg" />
    </Window.Background>
    <Grid Background="#C7FFFFFF">
        <ComboBox Height="31" HorizontalAlignment="Left" Margin="51,55,0,0" Name="comboBox_SelectProcess" VerticalAlignment="Top" Width="177" />
        <Label Content="Выберете процесс:" Height="30" HorizontalAlignment="Left" Margin="65,19,0,0" Name="label1" VerticalAlignment="Top" />
        <Button Content="Ok" Height="34" HorizontalAlignment="Left" Margin="97,117,0,0" Name="button_Ok" VerticalAlignment="Top" Width="83" Click="button_Ok_Click" />
    </Grid>
</Window>
