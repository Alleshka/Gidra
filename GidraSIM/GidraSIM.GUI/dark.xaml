<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:GidraSIM.GUI">
    <Style TargetType="DockPanel">
        <Setter Property="Background" Value="#FF2D2D30" />
    </Style>
    <Style TargetType="local:DrawArea">
        <Setter Property="Background" Value="#252526" />
    </Style>
    <Style TargetType="local:TestComplexitySelectionDialog">
        <Setter Property="Background" Value="#252526" />
    </Style>
    <Style TargetType="local:TestProcedureSelectionDialog">
        <Setter Property="Background" Value="#252526" />
    </Style>
    <Style TargetType="local:TestResourceSelectionDialog">
        <Setter Property="Background" Value="#252526" />
    </Style>
    <Style TargetType="local:TestSubProcessSelectionDialog">
        <Setter Property="Background" Value="#252526" />
    </Style>
    <!--<Style TargetType="local:TestSubProcessSelectionDialog">
        <Setter Property="Background" Value="#252526" />
    </Style>-->
    <Style TargetType="TextBlock">
        <Setter Property="Foreground" Value="White" />
    </Style>
    <Style TargetType="ToolTip">
        <Setter Property="Background" Value="#252526"/>
    </Style>
</ResourceDictionary>